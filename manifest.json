{
  "manifest_version": 2,
  "name": "Relative Links To Absolute",
  "version": "0.4",
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "/popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "options_page": "/options/options.html",
  "permissions": ["storage"]
}